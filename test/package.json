{
  "name": "bedrock-zcap-storage-test",
  "version": "0.0.1-0",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test c8 npm test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly --reporter=text-summary --reporter=text npm test",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "@bedrock/core": "^6.0.1",
    "@bedrock/https-agent": "^4.0.0",
    "@bedrock/jsonld-document-loader": "^3.0.0",
    "@bedrock/mongodb": "github:digitalbazaar/bedrock-mongodb#mongo-driver-4-rc-fix-duplicate-error",
    "@bedrock/test": "^8.0.5",
    "@bedrock/zcap-storage": "file:..",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "klona": "^2.0.5",
    "uuid": "^8.3.2"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/zcap-storage/**"
    ],
    "exclude": [
      "node_modules/@bedrock/zcap-storage/node_modules/**"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  }
}
